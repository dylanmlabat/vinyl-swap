<%= form_for(@album) do |f| %>
  <%= f.label :artist %>:<br>
  <%= f.text_field :artist %><br><br>
  <%= f.label :title %>:<br>
  <%= f.text_field :title %><br><br>
  <%= f.label :release_year %>:<br>
  <%= f.text_field :release_year %><br><br>
  <%= f.label :genre %>:<br>
  <%= f.text_field :genre %><br><br>
  <%= f.submit 'Create Album' %>
<% end %>
